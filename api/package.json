{
  "name": "api-ver-system",
  "version": "1.0.0",
  "description": "API for app conections",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "migrations:generate": "sequelize-cli migration:generate --name",
