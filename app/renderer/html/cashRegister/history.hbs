<!-- Bootstrap -->
    <link href="cdn.datatables.net/1.10.20/css/jquery.dataTables.min.css">
<!-- Datatables -->
    
    <link href="../../css/datatables.net-bs/css/dataTables.bootstrap.min.css" rel="stylesheet">
    <link href="../../css/datatables.net-buttons-bs/css/buttons.bootstrap.min.css" rel="stylesheet">
    <link href="../../css/datatables.net-fixedheader-bs/css/fixedHeader.bootstrap.min.css" rel="stylesheet">
    <link href="../../css/datatables.net-responsive-bs/css/responsive.bootstrap.min.css" rel="stylesheet">
    <link href="../../css/datatables.net-scroller-bs/css/scroller.bootstrap.min.css" rel="stylesheet">

<!-- Custom CSS -->   

<!-- page content -->
<div class="col-md-12 col-sm-12 ">
                <div class="x_panel">
                  <div class="x_title">
                    <h2>Flujo de Efectivo</h2>
                    <ul class="nav navbar-right panel_toolbox">                      
                      <li onclick="window.close();"><a class="close-link"><i class="fa fa-close"></i></a>
                      </li>
                    </ul>
                    <div class="clearfix"></div>
                  </div>
                  <div class="x_content">
                      <div class="row">
                        <div class="history-table">
                          <div class="col-sm-12">
                            <div class="card-box table-responsive">
                    <table id="datatable-buttons" class="table table-striped table-compact dt-responsive nowrap table-bordered" style="width:100%;"  cellspacing="0">
                     <thead>
                        <tr>
                          <th>Fecha</th>
                          <th>Monto</th>
                          <th>Quién Retira</th>
                          <th>Operación</th>
                          <th>Observación</th>
                          <th>Caja</th>
                        </tr>
                      </thead>
                      <tbody id="tbody-cashFlow">
                        {{#each cashFlow}}
                        <tr>
                          <th>{{this.date}}</th>
                          <th>$ {{this.amount}}</th>
                          <th>{{this.emplooy.name}}</th>
                          <th>{{this.operation}}</th>
                          <th>{{this.observation}}</th>
                          <th>{{this.box}}</th>
                        </tr>
                        {{/each}}
                      </tbody>
                    </table>
                  </div>
                </div>
              </div>
              </div>
            </div>
          </div>
      </div>
    <script src="../../js/cashFlowHistory.js"></script>